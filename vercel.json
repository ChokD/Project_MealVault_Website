{
  "version": 2,
  "builds": [
    {
      "src": "Backend/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "Backend/public/images/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/images/(.*)",
      "dest": "/Backend/public/images/$1"
    },
    {
      "src": "/(.*)",
      "dest": "Backend/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}